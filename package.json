{
  "name": "centos7-nodejs",
  "homepage": "https://github.com/bucharest-gold/origin-s2i-nodejs",
  "repository": {
    "type": "git",
    "url": "https://github.com/bucharest-gold/origin-s2i-nodejs.git"
  },
  "version": "0.0.3",
  "description": "NodeJS images for OpenShift / Kubernetes, based on CentOS",
  "devDependencies": {
    "semver": "~5.1.0",
    "roi": "~0.14.1",
    "underscore": "~1.8.3"
  },
  "keywords": [
    "openshift",
    "aws",
    "docker",
    "ansible",
    "kube",
    "kubernetes",
    "centos"
  ],
  "contributors": [
    "Ryan Jarvinen (http://ryanjarvinen.com)",
    "Lance Ball <lball@redhat.com> (http://lanceball.com)"
  ],
  "scripts": {
    "start": "make all",
    "rebuild": "node hack/rebuild.js",
    "tag": "hack/tag.sh",
    "pub": "hack/publish.sh",
    "build": "make",
    "test": "make test",
    "clean": "docker rmi -f $(docker images |tr -s ' ' | grep -e 'centos7-s2i-nodejs\\|centos7-s2i-nodejs-candidate\\|centos7-nodejs' | cut -d' ' -s -f3)"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bucharest-gold/origin-s2i-nodejs/issues"
  }
}
